<Page
    x:Class="UWP_DWT.MainPage"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:UWP_DWT"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    Background="{ThemeResource ApplicationPageBackgroundThemeBrush}">
    <RelativePanel>
        <WebView x:Name="WebView1" RelativePanel.AlignTopWith="CameraButton" RelativePanel.AlignLeftWithPanel="True" RelativePanel.AlignRightWithPanel="True" RelativePanel.AlignBottomWithPanel="True"  LoadCompleted="WebView1_LoadCompleted" Margin="0,32,0,0"/>
        <Button x:Name="CameraButton" Content="Camera" Click="CameraButton_Click"/>
    </RelativePanel>
</Page>
